{
  "name": "@ssv/backend",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "files": [
    "dist/index.js",
    "dist/index.js.map"
  ],
  "engines": {
    "node": "22"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "serve": "node scripts/prepare-emulator-env.mjs && firebase emulators:start --only functions,hosting,firestore",
    "deploy": "firebase deploy --only functions",
    "set-secrets": "node scripts/set-secrets-from-env.mjs",
    "prepare-emulator": "node scripts/prepare-emulator-env.mjs",
    "strip-dev": "node -e \"const f='./.deployed/package.json';const j=JSON.parse(require('fs').readFileSync(f));delete j.devDependencies;require('fs').writeFileSync(f,JSON.stringify(j,null,2))\""
  },
  "dependencies": {
    "@ssv/providers": "workspace:*",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^7.0.0",
    "openai": "^6.21.0",
    "@anthropic-ai/sdk": "^0.74.0",
    "@google/generative-ai": "^0.24.0"
  },
  "devDependencies": {
    "@ssv/audit": "workspace:*",
    "@types/node": "^22.0.0",
    "eslint": "^9.39.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.0"
  }
}
